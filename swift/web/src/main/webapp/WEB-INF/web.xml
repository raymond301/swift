<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Swift</display-name>

    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>/WEB-INF/applicationContext.xml</param-value>
    </context-param>

    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>

    <servlet>
        <servlet-name>services</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <init-param>
            <param-name>contextConfigLocation</param-name>
            <param-value>/WEB-INF/servlet-context.xml</param-value>
        </init-param>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>SwiftAppService</servlet-name>
        <servlet-class>org.springframework.web.context.support.HttpRequestHandlerServlet</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>ReportUpdate</servlet-name>
        <servlet-class>org.springframework.web.context.support.HttpRequestHandlerServlet</servlet-class>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>ConfigurationService</servlet-name>
        <servlet-class>org.springframework.web.context.support.HttpRequestHandlerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileUploadServlet</servlet-name>
        <servlet-class>org.springframework.web.context.support.HttpRequestHandlerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CommonDataRequester</servlet-name>
        <servlet-class>org.springframework.web.context.support.HttpRequestHandlerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Feed</servlet-name>
        <servlet-class>org.springframework.web.context.support.HttpRequestHandlerServlet</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>Status</servlet-name>
        <servlet-class>org.springframework.web.context.support.HttpRequestHandlerServlet</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>DirectoryService</servlet-name>
        <servlet-class>org.springframework.web.context.support.HttpRequestHandlerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TimeReport</servlet-name>
        <servlet-class>org.springframework.web.context.support.HttpRequestHandlerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Benchmark</servlet-name>
        <servlet-class>org.springframework.web.context.support.HttpRequestHandlerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AminoAcidReport</servlet-name>
        <servlet-class>org.springframework.web.context.support.HttpRequestHandlerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModificationReport</servlet-name>
        <servlet-class>org.springframework.web.context.support.HttpRequestHandlerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AnalysisReport</servlet-name>
        <servlet-class>org.springframework.web.context.support.HttpRequestHandlerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FindProtein</servlet-name>
        <servlet-class>org.springframework.web.context.support.HttpRequestHandlerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SampleReport</servlet-name>
        <servlet-class>org.springframework.web.context.support.HttpRequestHandlerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Extras</servlet-name>
        <display-name>Extras</display-name>
        <description>Extra Swift features</description>
        <jsp-file>/extras.jsp</jsp-file>
    </servlet>

    <!--inserted manually -->
    <servlet-mapping>
        <servlet-name>SwiftAppService</servlet-name>
        <url-pattern>/start/Service</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportUpdate</servlet-name>
        <url-pattern>/report/reportupdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConfigurationService</servlet-name>
        <url-pattern>/configuration/ConfigurationService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUploadServlet</servlet-name>
        <url-pattern>/start/FileUploadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CommonDataRequester</servlet-name>
        <url-pattern>/start/CommonDataRequester</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Feed</servlet-name>
        <url-pattern>/feed</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Status</servlet-name>
        <url-pattern>/status/searches</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DirectoryService</servlet-name>
        <url-pattern>/start/DirectoryService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TimeReport</servlet-name>
        <url-pattern>/time-report</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Benchmark</servlet-name>
        <url-pattern>/benchmark</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AminoAcidReport</servlet-name>
        <url-pattern>/amino-acids</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModificationReport</servlet-name>
        <url-pattern>/mods</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AnalysisReport</servlet-name>
        <url-pattern>/analysis</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FindProtein</servlet-name>
        <url-pattern>/find-protein</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SampleReport</servlet-name>
        <url-pattern>/sample-report</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Extras</servlet-name>
        <url-pattern>/extras</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
          <servlet-name>services</servlet-name>
          <url-pattern>/service/*</url-pattern>
      </servlet-mapping>

    <servlet-mapping>
        <servlet-name>jsp</servlet-name>
        <url-pattern>*.jsp</url-pattern>
    </servlet-mapping>
</web-app>
