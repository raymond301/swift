<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.mayo.mprc.searchdb.dao" default-lazy="true">
    <class name="TempLocalizedModification" table="temp_localized_modification">
        <composite-id class="edu.mayo.mprc.fastadb.TempKey" access="field" name="tempKey">
            <key-property name="job" column="job" type="integer" />
            <key-property name="dataOrder" column="data_order" type="integer" />
        </composite-id>
        <property name="newId" column="new_id" type="integer" not-null="false" />

        <many-to-one name="modSpecificity" column="specificity_id" class="edu.mayo.mprc.unimod.ModSpecificity" />
        <property name="position" column="position" type="integer" />
        <property name="residue" column="residue" type="character" />
    </class>
</hibernate-mapping>