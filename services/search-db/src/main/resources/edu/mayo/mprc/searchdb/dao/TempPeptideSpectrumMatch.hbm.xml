<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.mayo.mprc.searchdb.dao" default-lazy="true">
    <class name="TempPeptideSpectrumMatch" table="temp_peptide_spectrum_match">
        <composite-id class="edu.mayo.mprc.fastadb.TempKey" access="field" name="tempKey">
            <key-property name="job" column="job" type="integer" />
            <key-property name="dataOrder" column="data_order" type="integer" />
        </composite-id>
        <property name="newId" column="new_id" type="integer" not-null="false" />

        <many-to-one name="peptide" column="identified_peptide_id" class="IdentifiedPeptide" not-null="true" />
        <property name="previousAminoAcid" column="previous_aa" not-null="true" type="character" />
        <property name="nextAminoAcid" column="next_aa" not-null="true" type="character" />
        <property name="bestPeptideIdentificationProbability" column="best_id_probability" not-null="true"
            type="double" />
        <component name="spectrumIdentificationCounts" class="SpectrumIdentificationCounts" lazy="false">
            <property name="numberOfIdentifiedSpectra" column="total_identified_spectra" not-null="true"
                type="integer" />
            <property name="numberOfIdentified1HSpectra" column="identified_1h_spectra" not-null="true"
                type="integer" />
            <property name="numberOfIdentified2HSpectra" column="identified_2h_spectra" not-null="true"
                type="integer" />
            <property name="numberOfIdentified3HSpectra" column="identified_3h_spectra" not-null="true"
                type="integer" />
            <property name="numberOfIdentified4HSpectra" column="identified_4h_spectra" not-null="true"
                type="integer" />
        </component>
        <property name="numberOfEnzymaticTerminii" column="num_enzymatic_terminii" not-null="true" type="integer" />
    </class>
</hibernate-mapping>