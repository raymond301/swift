<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.mayo.mprc.database">
    <class name="TestFile" table="file">
        <id name="id" type="integer" column="file_id">
            <generator class="native" />
        </id>
        <property name="file1" type="file" length="200" column="file1" />
        <property name="file2" type="file" length="200" column="file2" />
    </class>
</hibernate-mapping>