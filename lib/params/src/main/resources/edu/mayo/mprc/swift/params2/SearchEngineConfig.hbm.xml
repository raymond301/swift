<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="edu.mayo.mprc.swift.params2.SearchEngineConfig" table="search_engine" lazy="false">
    <id name="id" column="search_engine_id" type="int">
            <generator class="native"/>
        </id>

        <property name="code" type="string" length="15" not-null="true" column="code"/>
        <property name="version" type="string" length="50" not-null="true" column="version"/>
    </class>
</hibernate-mapping>